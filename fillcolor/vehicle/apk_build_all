
#! /bin/sh

filepath=$(cd "$(dirname "$0")"; pwd)
echo $filepath
cd $filepath
cd ../../script

python copy_config.py $filepath icon 
python clean_screenshot.py $filepath 
python apk_build_clean.py 
python apk_build_all.py $filepath


python copy_config.py $filepath iconhd
python clean_screenshot.py $filepath 
python apk_build_clean.py 
python apk_build_all.py $filepath

